From 8328b9f3a8690ded00e0ff6c2844a4c216af12b9 Mon Sep 17 00:00:00 2001
From: fengmushu <fengmushu@gmail.com>
Date: Sun, 20 May 2018 03:15:32 +0800
Subject: [PATCH] fixup VR-H6 dts gmac power gpio

---
 .../boot/dts/allwinner/sun50i-h6-tempe-h6v2.dts    | 117 ++++++++++++++-------
 1 file changed, 79 insertions(+), 38 deletions(-)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-h6v2.dts b/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-h6v2.dts
index 329a05e..f7facc0 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-h6v2.dts
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-h6v2.dts
@@ -26,19 +26,32 @@
 	leds {
 		compatible = "gpio-leds";
 		pwr {
-			label = "nanopi:green:pwr";
-			gpios = <&r_pio 0 4 GPIO_ACTIVE_HIGH>;
+			label = "power";
+			gpios = <&r_pio 1 0 GPIO_ACTIVE_LOW>;
 			default-state = "on";
 		};
 
-		status {
-			label = "nanopi:blue:status";
-			gpios = <&r_pio 0 7 GPIO_ACTIVE_HIGH>;
+		link {
+			label = "link";
+			gpios = <&r_pio 1 1 GPIO_ACTIVE_LOW>;
 			default-state = "on";
 		};
+
+		disk {
+			label = "disk";
+			gpios = <&r_pio 1 2 GPIO_ACTIVE_LOW>;
+			default-state = "on";
+		};
+	};
+
+	wifi_pwrseq: wifi_pwrseq {
+		compatible = "mmc-pwrseq-simple";
+		/* reset-gpios = <&r_pio 1 3 GPIO_ACTIVE_LOW>; /* PM3 */
+		post-power-on-delay-ms = <50>;
 	};
 
 	reg_gmac_2v5: gmac-2v5 {
+		regulator-always-on;
 		compatible = "regulator-fixed";
 		regulator-name = "vcc-gmac-2v5";
 		regulator-min-microvolt = <2500000>;
@@ -48,10 +61,13 @@
 		enable-active-high;
 	};
 
-	wifi_pwrseq: wifi_pwrseq {
-		compatible = "mmc-pwrseq-simple";
-		reset-gpios = <&r_pio 1 3 GPIO_ACTIVE_LOW>; /* PM3 */
-		post-power-on-delay-ms = <50>;
+	reg_usb_vbus: vbus {
+		compatible = "regulator-fixed";
+		regulator-name = "usb-vbus";
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+		/* startup-delay-us = <100>; */
+		/* gpio = <&r_pio 0 5 GPIO_ACTIVE_HIGH>; PL5 use gpio */
 	};
 };
 
@@ -59,17 +75,6 @@
 	cpu-supply = <&reg_dcdca>;
 };
 
-&dwc3 {
-	status = "okay";
-};
-
-&ehci0 {
-	status = "okay";
-};
-
-&ehci3 {
-	status = "okay";
-};
 
 &emac {
 	pinctrl-names = "default";
@@ -121,14 +126,6 @@
 	status = "okay";
 };
 
-&ohci0 {
-	status = "okay";
-};
-
-&ohci3 {
-	status = "okay";
-};
-
 &r_i2c {
 	pinctrl-names = "default";
 	pinctrl-0 = <&r_i2c_pins>;
@@ -244,11 +241,35 @@
 			};
 		};
 	};
+};
+
+&twi0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi0_pins>;
+	status = "okay";
+};
+
+&twi1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi1_pins>;
+	status = "okay";
+};
+
+&twi2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi2_pins>;
+	status = "okay";
+
+};
 
-	pcf8563: rtc@51 {
-		compatible = "nxp,pcf8563";
-		reg = <0x51>;
-		#clock-cells = <0>;
+&twi3 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi3_pins>;
+	status = "okay";
+
+	ac200: i2c@10 {
+		compatible = "ac200,cdc";
+		reg = <0x10>;
 	};
 };
 
@@ -258,14 +279,23 @@
 	status = "okay";
 };
 
-&usb2otg {
-	dr_mode = "peripheral";
+&ohci0 {
 	status = "okay";
 };
 
-&usb2phy {
-	usb0_vbus-supply = <&reg_cldo1>;
-	usb3_vbus-supply = <&reg_cldo1>;
+&ehci0 {
+	status = "okay";
+};
+
+&dwc3 {
+	status = "okay";
+};
+
+&ohci3 {
+	status = "okay";
+};
+
+&ehci3 {
 	status = "okay";
 };
 
@@ -273,7 +303,18 @@
 	status = "okay";
 };
 
+&usb2otg {
+	dr_mode = "peripheral";
+	status = "okay";
+};
+
+&usb2phy {
+	usb0_vbus-supply = <&reg_usb_vbus>;
+	usb3_vbus-supply = <&reg_usb_vbus>;
+	status = "okay";
+};
+
 &usb3phy {
-	phy-supply = <&reg_cldo1>;
+	phy-supply = <&reg_usb_vbus>;
 	status = "okay";
 };
-- 
2.14.2

