From 7a0bb344b4bbc09953b2f669e0de96544eb98c01 Mon Sep 17 00:00:00 2001
From: fengmushu <fengmushu@gmail.com>
Date: Mon, 7 May 2018 21:30:42 +0800
Subject: [PATCH 1/2] dts for i2c aw9523b added

---
 .../boot/dts/allwinner/sun50i-h6-tempe-a55.dts     | 113 +++++++++++++++------
 arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi       |  38 +++++++
 2 files changed, 118 insertions(+), 33 deletions(-)

diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-a55.dts b/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-a55.dts
index e41e4b55..77656c74 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-a55.dts
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h6-tempe-a55.dts
@@ -11,8 +11,8 @@
 #include <dt-bindings/gpio/gpio.h>
 
 / {
-	model = "TEMPE Lite 2";
-	compatible = "tempe,tempe-lite2", "allwinner,sun50i-h6";
+	model = "TEMPE a55";
+	compatible = "tempe,tempe-a55", "allwinner,sun50i-h6";
 
 	aliases {
 		ethernet0 = &emac;
@@ -26,41 +26,51 @@
 	leds {
 		compatible = "gpio-leds";
 		pwr {
-			label = "nanopi:green:pwr";
-			gpios = <&pio 3 0 GPIO_ACTIVE_LOW>;
+			label = "a55:white:pwr";
+			gpios = <&pio 3 0 GPIO_ACTIVE_LOW>; /* PD0 */
 			default-state = "on";
 		};
 
 		status {
-			label = "nanopi:blue:status";
-			gpios = <&pio 3 1 GPIO_ACTIVE_LOW>;
+			label = "a55:white:status";
+			gpios = <&pio 3 1 GPIO_ACTIVE_LOW>; /* PD1 */
+			default-state = "on";
+		};
+
+		wifi_2G {
+			label = "a55:white:wifi2g";
+			gpios = <&pio 3 2 GPIO_ACTIVE_LOW>; /* PD2 */
+			default-state = "on";
+		};
+
+		rj-45 {
+			label = "a55:white:rj-45";
+			gpios = <&pio 3 3 GPIO_ACTIVE_LOW>; /* PD3 */
 			default-state = "on";
 		};
 	};
 
 	wifi_pwrseq: wifi_pwrseq {
 		compatible = "mmc-pwrseq-simple";
-		reset-gpios = <&r_pio 1 3 GPIO_ACTIVE_LOW>; /* PM3 */
+		reset-gpios = <&r_pio 1 3 GPIO_ACTIVE_HIGH>; /* PM3 */
 		post-power-on-delay-ms = <50>;
 	};
+
+	reg_usb_vbus: vbus {
+		compatible = "regulator-fixed";
+		regulator-name = "usb-vbus";
+		// regulator-min-microvolt = <5000000>;
+		// regulator-max-microvolt = <5000000>;
+		/* startup-delay-us = <100>; */
+		gpio = <&pio 3 7 GPIO_ACTIVE_HIGH>; /* PD7 */
+		regulator-boot-on;
+	};
 };
 
 &cpu0 {
 	cpu-supply = <&reg_dcdca>;
 };
 
-&dwc3 {
-	status = "okay";
-};
-
-&ehci0 {
-	status = "okay";
-};
-
-&ehci3 {
-	status = "okay";
-};
-
 &emac {
 	pinctrl-names = "default";
 	pinctrl-0 = <&inner_rmii_pins>;
@@ -109,14 +119,6 @@
 	status = "okay";
 };
 
-&ohci0 {
-	status = "okay";
-};
-
-&ohci3 {
-	status = "okay";
-};
-
 &r_i2c {
 	pinctrl-names = "default";
 	pinctrl-0 = <&r_i2c_pins>;
@@ -240,26 +242,61 @@
 	};
 };
 
+&twi0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi0_pins>;
+	status = "okay";
+
+	aw9523b_1: i2c@5b {
+		compatible = "awinic,aw9523b";
+		reg = <0x5b>;
+
+		gpios = <&pio 3 19 GPIO_ACTIVE_LOW>; /* PD19 RESET_N */
+	};
+};
+
 &twi1 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&twi1_pins>;
 	status = "okay";
 };
 
+&twi2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&twi2_pins>;
+	status = "okay";
+
+	aw9523b_2: i2c@5b {
+		compatible = "awinic,aw9523b";
+		reg = <0x5b>;
+
+		gpios = <&pio 3 21 GPIO_ACTIVE_LOW>; /* PD21 RESET_N */
+	};
+};
+
 &uart0 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&uart0_ph_pins>;
 	status = "okay";
 };
 
-&usb2otg {
-	dr_mode = "peripheral";
+&ohci0 {
 	status = "okay";
 };
 
-&usb2phy {
-	usb0_vbus-supply = <&reg_cldo1>;
-	usb3_vbus-supply = <&reg_cldo1>;
+&ehci0 {
+	status = "okay";
+};
+
+&dwc3 {
+	status = "okay";
+};
+
+&ohci3 {
+	status = "okay";
+};
+
+&ehci3 {
 	status = "okay";
 };
 
@@ -267,7 +304,17 @@
 	status = "okay";
 };
 
+&usb2otg {
+	dr_mode = "peripheral";
+	status = "okay";
+};
+
+&usb2phy {
+	phy-supply = <&reg_usb_vbus>;
+	status = "okay";
+};
+
 &usb3phy {
-	phy-supply = <&reg_cldo1>;
+	phy-supply = <&reg_usb_vbus>;
 	status = "okay";
 };
diff --git a/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi b/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
index f0329389..8887d4ae 100644
--- a/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
+++ b/arch/arm64/boot/dts/allwinner/sun50i-h6.dtsi
@@ -203,9 +203,25 @@
 				function = "uart0";
 			};
 
+			twi0_pins: twi0-pd {
+				pins = "PD25", "PD26";
+				function = "i2c0";
+				drive-strength = <20>;
+				bias-pull-up;
+			};
+
 			twi1_pins: twi1-ph {
 				pins = "PH5", "PH6";
 				function = "i2c1";
+				drive-strength = <20>;
+				bias-pull-up;
+			};
+
+			twi2_pins: twi2-pd {
+				pins = "PD23", "PD24";
+				function = "i2c2";
+				drive-strength = <20>;
+				bias-pull-up;
 			};
 		};
 
@@ -480,6 +496,17 @@
 			#size-cells = <0>;
 		};
 
+		twi0: i2c@05002000 {
+			compatible = "allwinner,sun6i-a31-i2c";
+			reg = <0x05002000 0x400>;
+			interrupts = <GIC_SPI 4 IRQ_TYPE_LEVEL_HIGH>;
+			clocks = <&ccu CLK_BUS_I2C0>;
+			resets = <&ccu RST_BUS_I2C0>;
+			status = "disabled";
+			#address-cells = <1>;
+			#size-cells = <0>;
+		};
+
 		twi1: i2c@05002400 {
 			compatible = "allwinner,sun6i-a31-i2c";
 			reg = <0x05002400 0x400>;
@@ -490,5 +517,16 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 		};
+
+		twi2: i2c@05002800 {
+			compatible = "allwinner,sun6i-a31-i2c";
+			reg = <0x05002800 0x400>;
+			interrupts = <GIC_SPI 6 IRQ_TYPE_LEVEL_HIGH>;
+			clocks = <&ccu CLK_BUS_I2C2>;
+			resets = <&ccu RST_BUS_I2C2>;
+			status = "disabled";
+			#address-cells = <1>;
+			#size-cells = <0>;
+		};
 	};
 };
-- 
2.14.2

